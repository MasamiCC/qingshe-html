<script type="text/javascript">
	mui.init()
	mui("#goods_list").on('tap','.pj_span ',function(){
		var hasC=$(this).hasClass('pj_span_active');
		if (!hasC) {
			$(this).parent().find('span').removeClass('pj_span_active');$(this).addClass('pj_span_active')
		}
	})
	$(function(){
		$('#score').raty({ 
			path    : '../image/',
        	starOff : 'icon2.png',
        	starOn  : 'icon1.png',
        	starHalf: 'star-half.png',
        	score: function() {
			    return $(this).attr('data-score');
			},
			width:'100%'
		});
	})
</script>