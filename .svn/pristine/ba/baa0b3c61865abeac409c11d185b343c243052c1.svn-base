<script src="js/jquery-1.10.1.min.js" type="text/javascript" charset="utf-8"></script>
<script src="js/mui.pullToRefresh.js"></script>
<script src="js/mui.pullToRefresh.material.js"></script>
<script>
			mui.init();
			(function($) {
				//阻尼系数
				var deceleration = mui.os.ios?0.003:0.0009;
				$('.mui-scroll-wrapper').scroll({
					bounce: false,
					indicators: true, //是否显示滚动条
					deceleration:deceleration
				});
				$.ready(function() {
					//循环初始化所有下拉刷新，上拉加载。
					$.each(document.querySelectorAll('.mui-slider-group .mui-scroll'), function(index, pullRefreshEl) {
						$(pullRefreshEl).pullRefresh({
							down: {
				                callback: function(){
				                	var self=this;
				                	self.endPulldownToRefresh()
				                }
				            },
				            up: {
				                contentrefresh: '正在加载...',
				                callback: function(){
				                	console.log(1)
				                	var self=this;
				                	self.endPullupToRefresh(false)
				                }
				            }
						});
					});
					var createFragment = function(ul, index, count, reverse) {
						var length = ul.querySelectorAll('li').length;
						var fragment = document.createDocumentFragment();
						var li;
						for (var i = 0; i < count; i++) {
							li = document.createElement('li');
							li.className = 'mui-table-view-cell shop_item';
							li.innerHTML = '<div class="shop_item_i shop_img">'
										+		'<img src="image/shop_2.png" class="shop_logo">'
										+	'</div>'
										+	'<div class="shop_item_i shop_msg">'
										+		'<ul>'
										+			'<li class="shop_title">这是一家'+index+'店铺'+i+'</li>'
