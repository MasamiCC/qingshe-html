	function countDown(count){
		
		var timer=setInterval(function(){
			var text='';
			if(count>=0){
				var h=parseInt(count/3600);
				var m=parseInt((count%3600)/60);
				var s=parseInt((count%60)%60);
				text="任务倒计时 "+h+"h:"+m+"m:"+s+"s";
				count--
			}else{
				clearInterval(timer)
				text='';
			}
			mui('.qsh_time')[0].innerHTML=text;
		},1000)
	}
