}
.index_tui_shop_logo img{
	max-width: 100%;
}

.index_tui_pf{
	position: absolute;
	right: 5px;
	top: 5px;
	width: 3rem;
	background: rgba(34,25,16,0.9);
	color: #d4ac57;
	text-align: center;
	padding: 0 0.4rem  ;
	font-size: 0.95rem;
	overflow: hidden;
	/*display: none;*/
}
.index_tui_pf span{
	line-height: 1.7;padding: 0.2rem 0;display: block;
}
.index_tui_pf span:first-child{
	border-bottom: 1px solid #d4ac57;margin: 0 auto;
	
}
.index_tui_pf.active{
	display: block;
	-webkit-animation:fadeInUp 1.5s ease 1 both;
	-moz-animation:fadeInUp 1.5s ease 1 both;
	-ms-animation:fadeInUp 1.5s ease 1 both;
	animation:fadeInUp 1.5s ease 1 both;
}

	@-webkit-keyframes fadeInUp{
		0%{
			opacity:0;
		}
		100%{
			opacity:1;
		}
	}
	@-moz-keyframes fadeInUp{
		0%{
			opacity:0;
		}
		100%{
			opacity:1;
		}
	}
	@-ms-keyframes fadeInUp{
		0%{
			filter:alpha(opacity="0");
		}
		100%{
			filter:alpha(opacity="100");
		}
	}
	@keyframes fadeInUp{
		0%{
			opacity:0;
		}
		100%{
			opacity:1;
		}
	}
	
