		<script type="text/javascript">
			mui.init()
			$(function(){
				loadPingjia();
				loadBacTop(10);
			})
			function loadPingjia(){
				$(window).scroll(function(){
					var top=$(window).scrollTop();
					var height=$(window).height()
					$(".index_tui_pf").each(function(){
						var offsetTop=$(this).offset().top;
						if (offsetTop-top<=height) {
							$(this).addClass('active');
						}
					})
					if ($(document).height()-top==height) {
						loadMoreShop()
					}
				})
			}
			var shopCount=1;
			var shopEnd=false;
			function loadMoreShop(){
				if (!shopEnd) {
					
					
					if (shopCount>=2) {
						shopEnd=true;
						mui.toast('没有更多数据了')
					}else{
						for( var i =0;i<10;i++){
							var html='<div class="index_tui_item">'
									+	'<div class="index_tui_shop_title" >'
									+		'<img src="../image/tui_shop_2.png" class="index_shop_icon"/>'
									+		'<span>豪食间中西餐厅（星湖店）</span>'
									+	'</div>'
									+	'<div class="index_tui_shop_logo">'
									+		'<img src="../image/near_shop-03.png"/>'
									+		'<div class="index_tui_pf">'
									+			'<span>'
									+				'评分'
									+			'</span>'
									+			'<span>'
									+				'4.5'
									+			'</span>'
									+		'</div>'
									+	'</div>'
									+	'<div class="index_tui_shop_con">'
									+		'<h3>价值96元的香肠单人套餐，提供免费WiFi,提供免费停车位</h3>'
									+		'<p>'
									+			'南通市开发区 | 10.5KM | 西餐'
									+			'<span class="mui-pull-right to_see">进去看看</span>'
									+		'</p>'
									+	'</div>'
									+'</div>'
							$("#tuijian_list").append(html);
							loadPingjia()
						}
						shopCount++;
					}
					
				}else{
					mui.toast('没有更多数据了')
				}
			}
