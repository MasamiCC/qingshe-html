.tech_box>div:nth-child(3n+1) img {
	-webkit-animation: rotater 4s linear infinite;
	-moz-animation: rotater 4s linear infinite;
	-ms-animation: rotater 4s linear infinite;
	animation: rotater 4s linear infinite;
}

.tech_box>div:nth-child(3n+2) img {
	-webkit-animation: rotater2 5s linear infinite;
	-moz-animation: rotater2 5s linear infinite;
	-ms-animation: rotater2 5s linear infinite;
	animation: rotater2 5s linear infinite;
}

.tech_box>div:nth-child(3n) img {
	-webkit-animation: rotater2 4s linear infinite;
	-moz-animation: rotater2 4s linear infinite;
	-ms-animation: rotater2 4s linear infinite;
	animation: rotater2 4s linear infinite;
}

.tech_box>div:first-child img,
.tech_box>div:last-child img {
	-webkit-animation: none;
	-moz-animation: none;
	-ms-animation: none;
	animation: none;
}



/*评估结果*/

.evalute_nav {
	text-align: center;
	color: #333;
	padding: 1rem 0;
}

.evalute_title {
	font-size: 24px;
}

.evalute_content {
	margin-top: 1rem;
	font-size: 0.9375rem;
}

.evalute_content>p {
	height: 2rem;
	line-height: 2rem;
	margin-bottom: 0;
}

.evalute_content .evalute_color {
	color: #f8a120;
	margin-right: .5rem;
}


/*按钮区域*/

.btn_area {
	padding: 0 1.5rem;
	margin-top: 1.5rem;
}

.btn_area .btn {
	display: block;
	width: 100%;
	height: 3rem;
	line-height: 3rem;
	border-radius: 3rem;
	text-align: center;
	font-size: 1rem;
	background-color: #bbbbbb;
	color: #FFFFFF;
	position: relative;
}

.btn_area .btn.active {
	background: -webkit-linear-gradient(left, #ffcd48, #f8a120);
    background: -o-linear-gradient(right, #ffcd48, #f8a120);
    background: -moz-linear-gradient(right, #ffcd48, #f8a120);
    background: linear-gradient(to right, #ffcd48, #f8a120);
}


