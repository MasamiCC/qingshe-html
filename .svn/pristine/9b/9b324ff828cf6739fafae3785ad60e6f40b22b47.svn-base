<script type="text/javascript">
	$(function(){
		loadStar();
		 $( "#slider-range" ).slider({
	      range: true,
	      min: 0,
	      max: 500,
	      values: [ 75, 300 ],
	      slide: function( event, ui ) {
	        $("#slider-range a").eq(0).html(ui.values[ 0 ]);
	    	$("#slider-range a").eq(1).html(ui.values[ 1 ])
	      }
	    });
	    $("#slider-range a").eq(0).html($( "#slider-range" ).slider( "values", 0 ));
	    $("#slider-range a").eq(1).html($( "#slider-range" ).slider( "values", 1 ));
	    
	    $(".clear_sel").click(function(){
	    	$(this).parents('.sel_now').remove()
	    })
	})
	var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: true,
        loop : true,
        autoplay : 2000,
    });
	var loadStar=function(){
		$('.raty').raty({
			score: function() {
			    return $(this).attr('data-score');
			},
			readOnly:true,
			starOn:'../imgs/icon1.png',
			starOff:'../imgs/icon2.png',
			starHalf:'../imgs/icon3.png',
			size:50,
		})
	}
	$('#begin_time').datetimepicker({
        language:  'zh-CN',
        weekStart:2,
        todayBtn:  0,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		minView: 2,
		maxView: 2,
		forceParse: 0,
		format: 'yyyy-mm-dd'
    });
    $('#end_time').datetimepicker({
        language:  'zh-CN',
        weekStart:2,
        todayBtn:  0,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		minView: 2,
		maxView: 2,
		forceParse: 0,
		format: 'yyyy-mm-dd'
    });
</script>
