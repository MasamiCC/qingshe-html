	<script id="member" type="text/html">
		{{each items value i}}
		<li>
        <div class="acount_area">
          <div class="acount_show">
          	<img class="header_ipc" src="{{value.header_ipc}}" alt="">
              <p class="account_name">{{value.name}}</p>
              <p class="account_phone">{{value.phone}}</p>
          </div>
          <div class="account_menu">
              <a><img src="../static/img/account02.png" alt=""> 查看详情</a>
          </div>
        </div>
    </li>
		{{/each}}
