.shop_item{
	display: flex;
	padding: 7px 15px;
}
.shop_img{
	flex: 1;
	max-width: 80px;
	text-align: center;
}
.shop_logo{
	width: 80px!important;
}
.shop_msg{
	flex: 2;padding-left: 10px;
}
.shop_pro{
	color: #666;font-size: 0.8rem;
}
.shop_status{
	background: #bbb;color: #fff;font-size: 0.75rem;padding: 1px 2px;border-radius: 3px;
}
.shop_on{
	background: #1f9b53;
}
.shop_juli{
	float: right;color: #999;
}
.shop_pingfen img{
	width: 15px;
	display: inline-block;
	max-width: 15px;
	margin-top: 2px;
	margin-right: 1px;
}
