<script src="../../../js/mui.min.js"></script>
<script src="../../../js/jquery-1.10.1.min.js"></script>
<script type="text/javascript">
	mui.init({
	pullRefresh: {
		container: '#pullrefresh',
		up: {
			contentrefresh: '正在加载...',
			callback: pullupRefresh,
			auto:true
		}
	}
});
var count = 0;

/**
 * 上拉加载具体业务实现
 */
function pullupRefresh() {
	setTimeout(function() {
		mui('#pullrefresh').pullRefresh().endPullupToRefresh((++count > 2)); //参数为true代表没有更多数据了。
		var sharecontainer = document.body.querySelector('.popularize_container_con');
		var sharediv = document.body.querySelectorAll('.popularize_container');
		for (var i = sharediv.length, len = i + 2; i < len; i++) {
			var div = document.createElement('div');
			div.className = 'share_foot_item_item_content';
			//字符串
		  div.innerHTML = '<div class="popularize_container">'
						+		'<div class="popularize_container_top">'
						+			'<div class="popularize_title">'
						+				'<span class="popularize_container_top_item1">'
						+					'2018年8月'	
						+				'</span>'
						+				'<span class="popularize_container_top_item">'
						+					'收益：¥ 2000'
						+				'</span>'
						+			'</div>'
						+			'<div class="popularize_title_right">'
						+				'<span class="popularize_T_icon">已打款</span>'
						+			'</div>'
						+		'</div>'
						+		'<div class="popularize_container_bottom">'
						+			'<div class="popularize_container_bottom_item popularize_container_bottom_item2">'
						+				'<span class="button_icon memberimg"><img src="img/member.png"/></span>'
						+				'<span class="button_icon">推广会员：</span>'
						+				'<span class="button_icon">30人</span>'
						+			'</div>'
						+			'<div class="popularize_container_bottom_item popularize_container_bottom_item3">'
						+				'<span class="button_icon signShopimg"><img src="img/signShop.png"/></span>'
						+				'<span class="button_icon ">签约商户：</span>'
						+				'<span class="button_icon">7家</span>'
						+			'</div>'
						+		'</div>'
						+	'</div>'
						
			
		            
				sharecontainer.appendChild(div);
		}
	}, 1500);
}
</script>

