.luxury_con{
	margin: 0 ;font-size: 0;
}
.luxury_con ul{
	font-size: 0;
}
.luxury_item{
	display: inline-block;vertical-align: top;
	/*margin:3px ;*/
	font-size: 0;
	float: left;
	margin-bottom: 10px;
	cursor: pointer;
}
.luxury_item >div{
	margin: 0 8px 0 6px;
}
.item_start{
	position: absolute;width: 100%;background: rgba(0,0,0,0.2);top: 0;left: 0;z-index: 99;height: 100%;
	text-align: center;
	vertical-align: middle;
	display: flex;
	align-items: center;
	font-size: 0;
	line-height: 0;
}
.item_start img{
	display: table;
	vertical-align: middle;
	margin: 0 auto;
	cursor: pointer;
}
.item_img{
	position: relative;font-size: 0;text-align: center;
}
.item_img img{
	max-width: 100%;
}
.clear{
	clear: both;
}
.item_name{
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-text-overflow: ellipsis;
    color: #333;
    font-size: 14px;
    line-height: 2;
}
.item_intro{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-text-overflow: ellipsis;
    color: #666;
    font-size: 12px;
    line-height: 1;
}
.luxury_item:first-child .item_img >img{
	max-width: inherit;
	width: 100%;
	height: 100%;
}
@media only screen and  (max-width: 275px) {
	.luxury_item:first-child{
		display: none;
	}
	.luxury_item{
		width: 100%;
	}
}
@media only screen and  (min-width: 275px) {
	.luxury_item:first-child{
		display: none;
	}
	.luxury_item{
		width: 50%;
	}
}
@media only screen and  (min-width:480px) {
	.luxury_item:first-child{
		display: none;
	}
	.luxury_item{
		width: 33.3%;
	}
}

@media only screen and  (min-width: 800px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 53.9%;
	}
	.luxury_item{
		width: 22.4%;
	}
	.luxury_item:nth-child(3n+3){
		display: none;
	}
}
@media only screen and  (min-width: 900px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 53.5%;
	}
	.luxury_item{
		width: 22.7%;
	}
	.luxury_item:nth-child(3n+3){
		display: none;
	}
}
@media only screen and  (min-width: 1000px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 53.5%;
	}
	.luxury_item{
		width: 23.2%;
	}
	.luxury_item:nth-child(3n+3){
		display: none;
	}
}
@media only screen and  (min-width: 1100px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 43.6%;
	}
	.luxury_item{
		width:18.6%;
	}
	.luxury_item:nth-child(3n+3){
		display: inline-block;
	}
}
@media only screen and  (min-width: 1150px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 43.7%;
	}
	.luxury_item{
		width: 18.75%;
	}
}
@media only screen and  (min-width: 1200px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 43.5%;
	}
	.luxury_item{
		width: 18.6%;
	}
}
@media only screen and  (min-width: 1280px) {
	.luxury_item:first-child{
		display: inline-block;
		width: 43.3%;
	}
	.luxury_item{
		width: 18.7%;
	}
}
.luxury_title{
	color: #d6a43d;font-size: 14px;padding: 3px 0;position: relative;text-indent: 15px;
	margin: 10px auto;
}
.luxury_title:after{
	background:#d6a43d ;position: absolute;left: 5px;width: 5px;height: 100%;top: 0;content: '';
}
.luxury_ads ul{
	font-size: 0;line-height: 0;
}
.luxury_ads li{
	display: inline-block;width: 50%;
}
.luxury_ads img{
	max-width: 100%;
	margin: 0 2px;
	width: 99%;
}
.luxury_list{
	margin-bottom: 10px!important;
}
.video_banner img{
	max-width: 100%;
}
/*luxury_detail*/
.luxury-detail {
	max-width: 1280px;
	margin: 0 auto;
	font-size: 0;
	margin-top: 70px!important;
}

.luxury-left{
	width: 100%;
	display: inline-block;
	vertical-align: top;
}
.luxury-right{
	width: 30%;
	display: none;
	vertical-align: top;
}
@media only screen and (min-width: 896px)  {
	.luxury-right{
		width: 30%;
		display: inline-block!important;
		
	}
	.luxury-left{
		width: 70%;
	}
}
.luxury_tuijian_list{
	background: #fff;padding: 10px;margin: 5px 10px 0 5px;
}
.luxury_tuijian_ad{
	margin: 0 10px 0 5px;
}
.luxury_tuijian_ad img{
	max-width: 100%;width: 100%;
}
.luxury_tuijian_title{
	font-size: 14px;padding: 5px 5px 5px 0 ;border-bottom: 1px solid #eee;
}
.luxury_left_item{
	background: #fff;margin-bottom: 10px;
	margin-right: 5px;
	margin-left: 10px;
	padding: 10px;
}
.luxury-user{
	color: #333;font-weight: 800;line-height: 1;margin-bottom: 5px;
}
.luxury-time{
	float: right;color: #aaa;font-weight: normal;margin-right: 10px;
}
.luxury-pj_con{
	color: #666;line-height: 1.5;font-size: 14px;white-space:normal
}
.luxury_more{
	background: #eee;color: #333;text-align: center;margin: 10px 0;
	padding: 8px 0;
}
.pj_list{
	display: flex;
	align-items: flex-start;
	border-bottom: 1px solid #eee;
	padding: 8px 0;
}
.pj_list:last-child{
	border-bottom: 0px;
}
.pj_left{
	flex: 1;
	max-width: 70px;
	text-align: center;
}
.pj_left img{
	width: 50px;height: 50px;border-radius: 50px;
}
.pj_right{
	flex: 3;
}
.pingjia_title{
	color: #eebd4e;border-bottom: 2px solid #eebd4e;padding: 5px 10px;text-align: center;
}


.to_shop{
	background: #eebd4e;color: #fff;font-size: 14px;padding: 5px 15px ;border-radius: 20px;
}
.shop_banner{
	display: flex;
	align-items: center;
}
.shop_banner img{
	border-radius: 50px;
	width: 50px;
	height: 50px;
}
.shop_banner >div{
	display: inline-block;
	width: initial;
	flex: 2;
}
.shop_banner >div:first-child{
	max-width: 70px;
	text-align: center;
	flex: 1;
	min-width: 70px;
}
.shop_banner >div:last-child{
	width: 100px;
	flex: 1;
	text-align: right;
	min-width: 100px;
}
.shop_title{
	font-size: 15px;font-weight: 700;
		overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:1; 
}
.shop_intro{
	font-size: 13px;color: #999;
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
}
.luxury_tuijian{
	font-size: 0;margin: 5px  auto;position: relative;
}
.luxury_tuijian >div{
	display: inline-block;
	width: 50%;
	vertical-align: top;
}
.luxury_tuijian >div:first-child img{
	width: 98%;
	max-width: 100%;
}
.luxury_tuijian .shop_title{
    line-height: 1.2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: inherit;
    color: #333;
    font-size: 14px;
}
.luxury_tuijian .shop_intro{
	line-height: 1.3;
	margin: 3px auto;
	font-size: 12px;
}
.luxury_time{
	margin-top: 8px;color: #999;font-size: 12px;
	position: absolute;
	width: 50%;
	bottom: 3px;
}
.luxury_view{
	float: right;font-size: 12px;
}
.luxury_view i{
	margin-right: 3px;
}
.luxury_btn{
	background: none;
	border:1px solid #ddd;
	padding: 5px  8px;
	cursor: pointer;
}
.luxury-text {
	position: relative;
}
.luxury-text textarea{
	width: 100%;
	height: 80px;
	resize: none;
	border: 1px solid #ddd;
}
.luxury-text div{
	position: absolute;
	top: 30px;
	width: 100%;
	text-align: center;
	color: #eebd4e;
}
#my-video{
	width: 100%;
}
.luxury_cat{
	color: #eebd4e;
}
.luxury-text span{
	cursor: pointer;
}
