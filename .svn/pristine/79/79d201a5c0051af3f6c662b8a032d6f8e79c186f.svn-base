}
.tuijian_item{
	margin: 10px;position: relative;padding: 5px  0;
}
.tuijian_item:first-child{
	margin-top: 0;padding-top: 0;
}
.tuijian_item img{
	max-width: 100%;
}
.tuijian_s_s{
	position: absolute;
	right: 10px;
	top: 10px;
	background: rgba(0,0,0,0.3);
	color: #edb94a;
	padding:5px;
	text-align: center;
}
.tuijian_s_s span{
	display: block;
	border-bottom: 1px solid #edb94a;
	line-height: 2;
}
.tuijian_s_title {
	line-height: 2;font-size: 16px;
}
.tuijian_s_address{
	font-size: 12px;color: #666;line-height: 1.5;margin: 2px auto;
}
.tuijian_title{
	line-height: 3;text-indent: 10px;
}
.search_msg{
	display: block;border-bottom: 1px solid #eee;margin-bottom: 10px;color: #333;
}
@media only screen  and (max-width: 800px) {
	.shop_list_r{
		display: none;
	}
	.shop_list_c >div:last-child {
	    display: none;
	}
	.shop_list_c >div:first-child{
		width: 100%;
	}
}
@media only screen and (min-width: 800px)  {
	.shop_list_r{
		display: inline-block;
	}
}
