<script type="text/javascript">
	
	$(function(){
		$('.pingjia_re_btn').each(function(){
			$(this).bind({
				'click':function(){
					$(this).parent().next().show()
				}
			})
		})
		$(".pingjia_re_sub").each(function(){
			$(this).bind({
				'click':function(){
					var txt=$(this).prev().val();
					if (!txt) {
						alert('请输入回复内容');
						$(this).prev().focus();
					}else{
						$(this).parent().prev().html('商家回复 <span>回复时间：2017-01-12 12:30</span>')
						$(this).parent().removeClass('pingjia_form').html(txt);
						
					}
				}
			})
		})
	})
</script>
