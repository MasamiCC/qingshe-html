	height: 40px;line-height: 40px;font-weight: bold;margin-bottom: 9px; color: #333;
}
.txt_btn{
	background: #333;color: #fff;display: inline-block;padding: 6px 16px;cursor: pointer;border-radius: 8px;
	line-height: 20px;
	/*float: right;*/
	font-weight: lighter;font-size: 14px;
}
.total{
	float: right;font-size: 14px;
}
.total span{
	color:#dd1136;font-weight: bold;margin-left: 10px;
}

.goods_cat{
	display: inline-block;padding: 5px 10px;position: relative;border-radius: 4px;
	/*border: 1px solid #ccc;*/
	margin: 5px auto;
}
.goods_cat span{
	background-image: url(../img/close.png);
	background-repeat: no-repeat;
	background-position: center;
	display: inline-block;
	width: 16px;
	height: 16px;
	margin: 3px 0 4px 10px;
	float: right;
	cursor: pointer;
}
.pingjia_item{
	/*display: inline-block;*/
	border-bottom: 1px dashed #ccc;padding: 10px 0;
	width: 100%;
}
.pingjia_item:last-child{
	border-bottom: none;
}
.pingjia_person,.pingjia_con{
	display: inline-block;vertical-align: top;
}
.pingjia_person{
	text-align: center;line-height: 40px;
	/*width: 10%;*/
}

.pingjia_img img{
	width: 80px; height: 80px;margin-top: 10px;
}
