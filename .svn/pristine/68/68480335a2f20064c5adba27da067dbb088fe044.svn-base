<script type="text/javascript">

    //导出图片
    $(document).on('click','.export_img',function(){     
        var canvas = $("#main").find("canvas").first()[0];

        var imgsrc = getBase64Image(canvas);

        var url = imgsrc;
        var a = document.createElement('a');  
        var event = new MouseEvent('click');  
          
        a.download = '轻奢点评用户关系图';  
        a.href = url;  
          
        a.dispatchEvent(event); 
    })

    //canvas转base64
    function getBase64Image(img) {
      var canvas = document.createElement("canvas");
      canvas.width = img.width;
      canvas.height = img.height;

      var ctx = canvas.getContext("2d");
      ctx.drawImage(img, 0, 0, img.width, img.height);

      var dataURL = canvas.toDataURL("image/png");
      return dataURL
    }

    //得到main的宽和高
    var width  = $('#main').outerWidth();
    var height  = $('#main').outerHeight();

    //canvas大小的调整
    $(document).on('click','.change_ize span',function(){
        var status = $(this).attr('class');
        //判断是需要放大还是缩小
        if(status == 'add'){
            width = width + 500;
            height = height + 250;
        }else{
            //如果宽高都小于最低值
            if (width < 500 && height < 250) {
                return false;
            }
            width = width - 500;
            height = height - 250;
        }

        $('#main').css({
            width: width+ 'px',
            height: height+ 'px'
        })

        //清除原先的echarts
        myChart.dispose();

        //重新生成canvas
        myChart = echarts.init(document.getElementById('main'), 'macarons');
        myChart.setOption(option);
    })

</script>    
