			/*popover*/
			#popover,#popover1{
				position: fixed;
				width: 100%;
			    margin: 0 auto;
			    left: 0;
			    right: 0;
			    /*top: 30%;*/
			    overflow: hidden;
			    text-align: center;
			    background: #fff;
			    /*min-height: 80%;*/
			    height: auto;
			    bottom: 0;
			}
			.title{
