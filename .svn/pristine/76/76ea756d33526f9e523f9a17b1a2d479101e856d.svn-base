	<script>
		$(function(){
			var nScrollHight = 0; //滚动距离总长
			var nScrollTop = 0;  //滚动到的当前位置
			var nDivHight = $(".lay_body").outerHeight(); //需要被加载的对象

			var isLoad = false; //是否正在加载数据
			var isEnd = false; //是否为最后一页
			var time; //定义一个定时器

			//定义一个假的数据
			var data = {
				items:[
		            {name:"jack",phone:18},
		            {name:"tom",phone:19},
		            {name:"jerry",phone:20},
		            {name:"kid",phone:21},
		            {name:"jade",phone:22}
		        ]
			}

			//加载数据的函数
			var getData = function(){
				var promptHtml = $("<p class='prompt_line'>加载中...</p>")
	    	time = setTimeout(function(){
	      	$('.index_nav').append(promptHtml);
	      	//加载数据
				  var html = template("member", data);
				  $('.account_list').append(html);
				  isLoad = false;
				  //判断是否为最后一页
				  if(isEnd){
				  	$('.prompt_line').html('加载完成');
				  	isEnd = true;
				  }else{
				  	$('.prompt_line').remove();
				  }
	      },500);
	    }


			//监听滚动条滚到底部
			$(".lay_body").scroll(function(){
				if(time){
					clearTimeout(time)
				}
				nScrollHight = $(this)[0].scrollHeight;
	      nScrollTop = $(this)[0].scrollTop;
	      //如果滚动条滚到底部
	      if(nScrollTop + nDivHight >= nScrollHight && !isLoad && !isEnd){
	        getData();
	        isLoad = true;
	      };
			})
		})
	</script>
