<script type="text/javascript">
	$(function(){
		
		$(".cat_list").scroll(function(){
			var a='';
			var res='';
			var id='';
			var ltop=$(".cat_list").offset().top;
			var lheight=$(".cat_list").height();
			console.log(lheight)
			console.log(ltop);
			var arr=[];
			$('.shopping_cat_title').each(function(e){
				var top=$(this).offset().top-ltop;
				console.log(top)
				if (top>-100&&top<100) {
					id=$(this).parent().attr('data-id');
					$(".shopping_cat li").removeClass('active')
					$("#cat_"+id).addClass('active')
				}else{
					
				}

			})

		})
		$(".shopping_shop_type li").each(function(){
			$(this).bind({
				'click':function(){
					$(this).addClass('active').siblings().removeClass('active')
				}
			})
		})
		$('.begin_time').click(function(){
			$('#begin_time').datetimepicker('show');
		})
		$('.end_time').click(function(){
			$('#end_time').datetimepicker('show');
		})
	})
	
var begintime_s = new Date();
var begintime_e = new Date(+begintime_s+1000*60*60*24*7);
var endtime_s = new Date(+begintime_s+1000*60*60*24*1);
var endtime_e=new Date(+begintime_s+1000*60*60*24*8);
//console.log(begintime_s)
//console.log(begintime_e)
//console.log(endtime_s)
//console.log(endtime_e)
var beginsmonth=begintime_s.getMonth()+1;
	beginsmonth=beginsmonth>=10?beginsmonth:"0"+beginsmonth;
var beginsday=begintime_s.getDate()
	beginsday=beginsday>=10?beginsday:"0"+beginsday;
	
var endsmonth=endtime_s.getMonth()+1;
	endsmonth=endsmonth>=10?endsmonth:"0"+endsmonth;
var endsday=endtime_s.getDate()
	endsday=endsday>=10?endsday:"0"+endsday;
	$("#begin_time").val(begintime_s.getFullYear()+'-'+beginsmonth+'-'+beginsday); 
	$("#end_time").val(endtime_s.getFullYear()+'-'+endsmonth+'-'+endsday); 


$("#begin_time").datetimepicker({  
    language : 'zh-CN',  
    weekStart : 1,  
    todayBtn : 0,  
    autoclose : 1,  
    //todayHighlight : 1,  
    startView : 2,  
    minView: "month",  
    startDate:begintime_s,
    endDate:begintime_e,
    format: 'yyyy-mm-dd',  
    forceParse : 0  
}).on('hide', function(event) {  
    event.preventDefault();  
    event.stopPropagation(); 
    var startTime = new Date(event.date); //返回的时间
    var endTime=new Date(+startTime+1000*60*60*24*1);//一天之后的时间
    $("#end_time").datetimepicker('setStartDate',endTime); 
	var month=endTime.getMonth()+1
	month=month>=10?month:"0"+month;
	var day=endTime.getDate();
	day=day>=10?day:"0"+day;
	$("#end_time").val(endTime.getFullYear()+'-'+month+'-'+day); 
	$('#end_time').datetimepicker('update');
	loadCartData();
});

$("#end_time").datetimepicker({  
    language : 'zh-CN',  
    weekStart : 1,  
    todayBtn : 0,  
    autoclose : 1,  
    startView : 2,  
