<script type="text/javascript">
	//	 图片轮播开始

window.setInterval("changePicture()",1500);
var index = 1;
var stopTimer = false;
function changePicture(){
    //图片切换
    if(stopTimer){
        return;
    }
    //隐藏所有的图片
    $(".img-box img").css("display","none");
    var selector = ".img-box .p"+index;
    //显示相应的图片
    $(selector).css("display","block");
    //$(selector).show(1000);
    //所有按钮背景变回原色
    $(".page-con li").css("background","white");
    //相应按钮变背景颜色
    $(".page-con .p"+index).css("background","#FF3F3F");
    index ++;
    //如果到了最后一个，回到第一个
    if(index==4){
        index = 1;
    }
    //按钮颜色变化
}
$(function(){
    //显示第一张图片
    $(".img-box .p1").css("display","block");
    //第一个按钮变背景颜色
    $(".page-con .p1").css("background","#FF3F3F");
    //鼠标放在page_con 里面的li事件
    $(".page-con li").mouseover(function(){
        stopTimer = true;
        index = $(this).html();
        var selector = ".img-box .p"+index;
        $(".img-box img").css("display","none");
        //显示相应的图片
        $(selector).css("display","block");
        //所有按钮背景变回原色
        $(".page-con li").css("background","white");
        //按钮背景变颜色
        $(this).css("background","#FF3F3F");
    });
    $(".page-con li").mouseout(function(){
        stopTimer = false;
    });
});
    
//   图片轮播结束

	 $(function(){
      $(".shadow").mouseover(function(){
        $(this).css({"box-shadow":"2px 2px 3px #BFBFBF","border":"1px solid #DADADA"});
        
    }).mouseout(function(){
        $(this).css({"box-shadow":"2px 2px 3px white","border":"1px solid white"});
    });
    
    
  
    
    });

</script>