*{
				padding: 0px;
				margin: 0px;
				list-style: none;
			}
			body{
				background: #f4f4f4;
			}
			header .mui-title{
				color: #fff;
				font-size: 1rem;
			}
			header .mui-icon-left-nav{
				color: #fff;
			}
			.main{
				background: url(../image/heartbank_bg.png);
				background-size: 100% 100%;
				padding: 0 15px;
			}
			.btns div{
				display: inline-block;
				font-size:0.875rem ;
				border: 1px solid #fff;
				border-radius: 5px;
				color: #fff;
				padding:  0.5rem 0.9375rem;
				/*margin: 0 6%;*/
			}
			.sbtn{
				float: right;
			}
			.btns{
				padding: 0 12% 1.875rem 12%;
			}
			.black{
				display: inline-block;
				width: 0.157rem;
				height: 0.875rem;
				background: #333;
			}
			.charity{
				background: #fff;
				margin-top: 0.625rem;
			}
			.charity_title{
				line-height: 2.625rem;
				height: 2.625rem;
				border-bottom: 1px solid #ededed;
				padding: 0 0.9375rem;
				font-size: 1rem;
			}
			.uls{
				padding: 0 0.625rem;
			}
			.firstul{
				font-size: 0;
				line-height: 0;
				text-align: center;
			}
			.firstul li{
				
				/*font-family: 0;*/
			}
			.firstli{
				display: inline-block;
				width: 25%;
				font-size: 0.875rem;
				color: #333
			}
			.icons,.fonts{
				height: 2rem;
				line-height: 2rem;
			}
			.uls{
				padding: 0.9rem 0;
			}
			.coup1{
				font-size: 0.8125rem;
				padding-bottom: 0.625rem;
				color: #999;
			}
			.coup2{
				font-size: 1.625rem;
				color: #333;
			}
			.coupon{
				text-align: center;
			}
			.coupuls{
				padding: 1.5625rem 0 0.9375rem 0;
			}
			.pointuls{
				text-align: center;
				padding: 1.6875rem 0;
			}
			.point1{
				font-size: 1.25rem;
				padding-bottom: 0.625rem;
			}
			.point2{
				font-size: 0.8125rem;
				color: #999;
			}
			.order a{
				color: #f8a120;
				font-size: 0.9375rem;
			}
			.get{
				color: #fff;
				font-size: 0.875rem;
				display: inline-block;
				padding: 10px 30px;
				background: #f8a120;
				border-radius: 6px;
				margin-bottom: 15px;
			}
			.icons img{
				width: 16px;
			}

			
			.hb_main_con{
				position: absolute;
			    width: 100%;
			    top: 0;
			    left: 0;
			    right: 0;
			    bottom: 0;
			    overflow: auto;
			}
			#popover {
				position: absolute;
			    height: 77%;
    			width: 75%;
			    margin: 0 auto;
			    left: 0;
			    right: 0;
			    top: 11%;
			    bottom: 0;
			    overflow: hidden;
			    text-align: center;
			    min-height: 428px;
			}
			.popover_middle img{
				max-width: 100%;
			}
			.mui-popover{
				background: none;
				box-shadow:none;
			}
			
			.mui-popover .mui-scroll-wrapper {
			    /*margin: 7px 0;*/
			    border-radius: 0px;
			}
			.shai_fonts{
				position: absolute;
				top: 37%;
				left: 0;
				right:0;
				text-align: center;
				margin: 0 auto;
				font-weight: bold;
				font-size: 1.75rem;
				color: #fff;
			}
			
			
			#myCanvas{margin: 0 auto;}
			#img{max-width: 80%;}
			#ringoImage{display: none;}

