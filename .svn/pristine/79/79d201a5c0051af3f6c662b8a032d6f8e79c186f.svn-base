.no_data{
	text-align: center;line-height: 5;font-size: 14px;
}
.refresh{
	max-width: 45px;margin: 0 auto;display: block;margin-top: 50px;cursor: pointer;
}
.refresh.active,.refresh:hover{
animation: myfirst 2s linear infinite;
-moz-animation: myfirst 2s linear infinite;	/* Firefox */
-webkit-animation: myfirst 2s linear infinite;	/* Safari 和 Chrome */
-o-animation: myfirst 2s  linear infinite;	/* Opera */

}
@keyframes myfirst
{
	from {transform:rotate(0deg);}
	to {transform:rotate(360deg);}
}

@-moz-keyframes myfirst /* Firefox */
{
	from {-moz-transform:rotate(0deg); }
	to {-moz-transform:rotate(360deg); }
}

@-webkit-keyframes myfirst /* Safari 和 Chrome */
{
	from {-webkit-transform:rotate(0deg);}
	to {-webkit-transform:rotate(360deg);}
}

@-o-keyframes myfirst /* Opera */
{
	from {-o-transform:rotate(0deg); }
	to {-o-transform:rotate(360deg); }
}


