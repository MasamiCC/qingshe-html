*{
	padding: 0;
	margin: 0;
	list-style: none;
	outline: none;
	font-family: "微软雅黑";
	font-size: 14px;
}
/*a{
	text-decoration: none;
	color: #edb94a;
	font-size: 15px!important;
	padding: 0 3px;
}*/
.changecityheader a{
	color: #666;
	font-size: 14px!important;
}
.changecityheader a:hover{
	text-decoration: underline;
	background: none;
}
.changecitymiddleheader{
	padding: 20px 20px 5px 20px;
}
.pinyin{
	display: inline-block;background: url(../imgs/pinyinbg.png) no-repeat;
	line-height: 2.4;
	padding: 0 14px 5px 14px;
	color: #fff;
}
.changecitymiddle a{
	color: #999;text-decoration: none;
	font-weight: normal;
}
.changecitymiddle a:hover{
	color: #edb94a;
}
.changecitymain{
    width: 100%;
    font-weight: 700;
}
.changecitymain_container{
	 border: 1px solid #ddd;
	 box-shadow: 0 0 1px #d3ebee;
	 padding-bottom: 60px;
}
.changecitymain>div{
		max-width: 1280px;
		margin: 0 auto;
}
.inline_container{
	line-height: 0;
	font-size: 0;
}
.inline_container>div{
	display: inline-block;
}
.changecityleft{
	width: 7.5%;
	line-height: 1.5;
	font-size: 15px;
	vertical-align: top;
}
.changecityright{
	width: 92.5%;
	line-height: 1.5;
	vertical-align: top;
}
.changecityleft1{
	width: 15%;
	line-height: 1.5;
	font-size: 15px;
	vertical-align: top;
}
.changecityright1{
	width: 85%;
	line-height: 1.5;
	vertical-align: top;
}
.changecityright1 a,.changecityright1 a:hover{
	color: red;text-decoration: underline;
}
.changecityul li{
	display: inline-block;
	/*width: 4.5em;*/
	margin: 0 50px 0 0;
}
.changecitymiddlemiddlerightul li{
	display: inline-block;
	padding: 0 2px 1px;
    margin: 3px 10px;
}
.changecityheader1left,.changecityheader1right{
	width: 50%;
}
.changecityheader1right{
	line-height: 1.5;
	vertical-align: top;
}
.changecityheader>div{
	padding: 20px;border-bottom: 1px solid #E5E5E5;
}
.select{
	float: right;
	font-weight: 100;
	color: #FFF;
	background: #edb94a;
	padding: 2px 10px;
	cursor: pointer;
}
.changecitymiddlemiddle{
	padding: 6px 10px 6px 30px;
	border-top: 1px solid #fff;
	border-bottom:1px solid #fff;
}
.changecitymiddlemiddleleft{
	width: 4.5%;
	vertical-align: middle;
}
.changecitymiddlemiddleright{
	width:95.5% ;
	line-height: 20px;
	vertical-align: middle;
}
.headericon{
	display: inline-block;
	color: #666;
	padding: 16px 11px;
	background: #eee;
	border: 1px solid #ddd;
	margin-top: 0px;
}
.changecitymiddlemiddle:hover{
	border-top: 1px solid #E5E5E5;
	border-bottom:1px solid #E5E5E5;
	background: #F7F7F7;
}

.changecityheader2  a{
	color: #333;text-decoration: none;
}
.changecityheader2  a:hover{
	color: #edb94a;text-decoration: underline;
}

.changecityheader1right input[type=text]{
	border: 1px solid #ccc;
	height: 35px;
	text-indent: 5px;
	color: #999;
	background: none;
}
