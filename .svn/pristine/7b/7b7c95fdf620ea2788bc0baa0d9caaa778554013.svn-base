}
.pingjia {
    font-size: 0.75rem;
    color: #333;
}
.pingjia img{
	max-width: 5rem;
}
.pingjia p{
	color: #333;margin-bottom: 5px;
	    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    white-space: normal;
}
.user_lev {
    color: #ff9600;
}
.pingjia_time {
	color: #999;font-weight: normal;
}




