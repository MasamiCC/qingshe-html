	display: block;
	border-bottom: 1px solid #d4ac57 ;
	font-size: 0.9rem;
}
.gprs{
	display: flex;padding: 8px 10px!important;
}
.gprs_l{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-text-overflow: ellipsis;
    font-size: 0.75rem;
    flex: 3;
    border-right: 1px solid #ddd;
    margin-right: 0.5rem;
}
.gprs_l i{
	color: #d4ac57;
}
