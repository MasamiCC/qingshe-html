.mui-content {
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
    bottom: 70px;
    overflow: auto;
}
	#myblackcardPopover {
		position: absolute;
	    height: 52%;
		width: 75%;
	    margin: 0 auto;
	    left: 0;
	    right: 0;
	    top: 25%;
	    overflow: hidden;
	    text-align: center;
	    min-height: 347px;
	    background: #fff;
	}
	.popover_middle img{
		max-width: 35%;
	}
	.popover_middle{
		line-height: 1;
	}
	.popover_bottom_fonts{
		font-size: 0.9375rem;
		color: #000;
	}
	.popover_title{
		color: #868686;
		font-size: 1rem;
		background: #fff;
		height: 45px;
		line-height: 45px;
		position: relative;
	}
	.mui-popover .mui-scroll-wrapper{
		margin: 0;
	}
	.popover_bottom_icons:first-child{
		margin-top: 40px;
		background: #333;
		color: #fff;
	}
	.popover_bottom_icons{
		display: inline-block;
		/*padding: 11px 55px;*/
		width: 166px;
		height: 38px;
		line-height: 38px;
		border-radius: 50px;
		border: 1px solid #333;
		margin-bottom: 20px;
	}
	#myblackcardPopover .mui-icon{
		font-size: 40px;
	}
	.myblackcardPopover_font{
		text-align: center;
	}
	.popover_title{
		text-align: right;
	}
	.popover_bottom_icons a{
		color: inherit;
	}
