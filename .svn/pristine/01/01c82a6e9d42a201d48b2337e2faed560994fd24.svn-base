*{
	padding: 0;
	margin: 0;
}
.mui-content{
	background: -webkit-gradient(linear, 0% 0%, 0% 100%,  from(#ea6270), to(#d80c53));
	background: -webkit-linear-gradient(#ea6270, #d80c53);
	background: -moz-linear-gradient(#ea6270, #d80c53);
	background: -o-linear-gradient(#ea6270, #d80c53);
	background: linear-gradient(#eb6a6f, #d80c53);
}
body{
	background-color:#d80c53;
}
.main{
	background: url(../image/signin2_bottom.png) no-repeat 41% 99%;
    background-size: 121% 53%;
}
.main_top img{
	max-width: 100%;
	line-height: 0;
	font-size: 0;
}
/*.marin_top{
	background: url('../image/signin2_banner.png');
	background-size: 100% 100%;
	height: 50%;
	min-height: 280px;
}*/
.main_middle_top img{
	width: 55%;
}
.main_middle_mian{
    position: absolute;
    top: 24px;
  	margin: 0 auto;
  	left: 0;
  	right: 0;
    background: url(../image/qiandaobottom.png) no-repeat center center;
    background-size: 45% 100%;
    padding-bottom: 4%;
}
.main_middle{
	height: 194px;
	text-align: center;
	position: relative;
}
.main_middle_mian img{
	width: 45%;
}
.main_bottom_top img{
	width: 37.5%;
}
.main_bottom_top{
	padding-top: 5%;
	text-align: center;
}
.main_bottom_mian{
	background: url('../image/rule_bg.png') no-repeat center;
	background-size: 89% 89%;
	padding: 15% 14% 15% 18%;
	font-size: 0.75rem;
	color: #e1547b;
}
.main_bottom{
	padding-bottom: 20px;
}
.daynum{
    position: relative;
    color: #fff;
    background: #d70050;
    font-size: 40px;
    font-family: Lucida;
    border-radius: 8px;
    width: 38px;
    height: 38px;
    margin: 0 5px;
    vertical-align: middle;
    display: inline-block;
    line-height: 38px;
}
.num{
	position: absolute;
	left: -10px;
    right: 0;
}
.main_middle_mian_font{
	font-size: 18px;
	color: #d1003b;
	line-height: 2.3;
	padding-top: 6%;
}
.greygeticon{
	color: #fff;
	font-size: 1rem;
	padding: 5px 15px;
	background: #b6b6b6;
	border-radius: 20px;
	line-height: 2;
}
.greygeticonline{
	margin-top: 10px;
}
.red{
	background: #D70050;
}
#myPopover div{
	max-width: 71%;
}
#myPopover2{
	top: 0;bottom: 0;
	width: 100%;
	background: none;
	text-align: center;
	padding-top: 38%;
	position: fixed;
}
#myPopover2 div{
	margin: 0 auto;
}
#myPopover2 .mui-icon{
	margin-top: 50px;
}
#myPopover2 .mui-icon{
	margin-top: 30px;
}
.popover_top{
    background: -moz-linear-gradient(top, #993e8e, #dc2e47);
	background: -o-linear-gradient(top,#993e8e, #dc2e47);
	background: -webkit-gradient(linear, 100% 100%, 0% 100%, from(#993e8e), to(#dc2e47));
    padding: 15px;
    height: 60%;
    border-radius: 6px;
    overflow: auto;
}
.popover_bom{
	text-align: center;
}
.mui-icon-close{
	font-size: 38px;
}
.popover_middle{
	background: -moz-linear-gradient(top, #993e8e, #dc2e47);
	background: -o-linear-gradient(top,#993e8e, #dc2e47);
	background: -webkit-gradient(linear, 100% 100%, 0% 100%, from(#993e8e), to(#dc2e47));
	color: #fff;
	line-height: 1.5;
	border-radius: 20px;
	padding: 15px;
}
#myPopover div{
	width: 84%;
	border-radius: 20px;
}

.mui-popover{
	box-shadow: 0 0 15px rgba(0,0,0,0.75);
}
.myPopover_top{
	position: relative;
	width: 88%;
	border-radius: 20px;
}
.popover_header1{
   position: absolute!important;
    top: -26%;
    left: 13%;
    z-index: 5;
}
.popover_header2{
    position: absolute!important;
    z-index: 5;
   	top: -19%;
    left: 10%;
}
.popover_bottom{
    position: absolute!important;
    top: 70%;
    left: -5%;
    z-index: -1;
}
.popover_header1 img{
	width: 50%;
	height: 50%;
}
.popover_header2 img,.popover_bottom img{
	max-width: 100%!important;
}
.couponcon{
	background: url(../image/quan.png) no-repeat center center;
    background-size: 76% 89%;
    position: relative;
    left: 0;
    right: 0;
    margin: 20px auto!important;
	
}
.coupon_left{
	font-size: 0;
	line-height: 0;
	padding-top: 10px;
	padding: 7% 0 7% 17%;
}
.coupon_left>div{
	display: inline-block;
}
.coupon_left_font{
	font-size: 0.875rem;
	color: #D70050;
	line-height: 1;
	vertical-align: middle;
}
.bigf{
	font-size: 50px;
}
.couponcon{
	text-align: left;
}
.quanwei{
	position: absolute;
	right: 3%;
	top: 1px;
}
.quanwei img{
	width: 88%;
}
.coupon_right_font{
	font-size: 0.75rem;
	color: #866144;
	line-height: 1.7;
	text-align: center;
	vertical-align: middle;
}
.geticonquan{
    display: inline-block;
    width: 52px;
    height: 52px;
    border-radius: 50%;
    background: #c6c2c2;
    font-size: 0.625rem;
    line-height: 52px;
    text-align: center;
    position: absolute;
   	top: 20%;
    left: 24%;
}
.redquan{
	background: #e8434f;
}
@media only screen and (max-width: 320px) {
	.couponcon {
	    background: url(../image/quan.png) no-repeat center center;
	    background-size: 82% 89%;
	}
	.coupon_left{
		padding: 7% 0 7% 14%;
	}
	.quanwei {
	    right: -9%;
	    top: 2px;
	}
	.quanwei img {
	    width: 77%!important;
	}
	.geticonquan{
		left: 20%;
	}
}
