	width: 5.2rem;
	max-width: 5.2rem;
	height: 5.2rem;
	line-height: 5.2rem;
}
.line_p{
	font-weight: bold;
	font-size: 1rem;
	color: #000;
	position: absolute;
	bottom: .3rem;
	right: .8rem;
}
.line_p small{
	color: #999;
	font-size: 0.70rem;
}
.last_line_title{
	font-size: 0.85rem;margin-top: 2px;font-weight: normal;
}
.last_line_intro{
	font-size: 0.75rem;line-height: 1.5;
}
#lasted_line ul:before,#lasted_line ul:after{
	height: 0;
}
#lasted_line li:after{
	left: 0!important;
}
#lasted_line li.mui-active{
	background: #fff!important;
}
#lasted_line a.mui-active{
	background: #fff!important;
}
#lasted_line li:after{
	background-color: #efefef;
}
#fujin_tui .cat_sel_title{
	margin-bottom: 0;
}
.shop_detail_pj_item {
    display: flex;
    padding-top: 10px;
    margin: 0 0.5em;
}
.shop_detail_pj_left {
    flex: 1;
    width: 60px;
    max-width: 60px;
    text-align: left;
}
.shop_detail_pj_left img {
    max-width: 45px;
    border-radius: 50px;
}
.shop_detail_pj_right {
    flex: 2;
    border-bottom: 1px solid #efefef;
    padding-bottom: 0;
}
.shop_detail_pj_item:last-child .shop_detail_pj_right{
	border-bottom: none;
}
.user {
    font-size: 0.85rem;
