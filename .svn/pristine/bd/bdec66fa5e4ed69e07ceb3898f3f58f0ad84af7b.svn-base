/*
 * 10.11修改
 */
*{
	box-sizing: border-box;
}

p{
	margin: 0 0 0 0;
}

.container_nav{
	width: 100%;
	height: 100%;
	padding: 20px;
	background-color: #f4f4f4;
}
			
/*基本信息*/
.info_nav{
	background-color: #fff;
	padding: 25px 30px;
	box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.1);
    -moz-box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.1);
    -webkit-box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.1);
    overflow: hidden;
}

.info_nav:first-child{
	margin-bottom: 25px;
}

.info_nav .img_info{
	float: left;
	width: 160px;
	height: 160px;
	margin-right: 50px;
}


.info_nav .img_info img{
	width: 100%;
	height: 100%;
	border-radius: 100%;
}

.info_nav .person_info{
	min-width: 400px;
	float: left; 
}

.info_nav .info_complet{
	width: 100%;
	line-height: 40px;
	margin-bottom: 5px;
}

.info_complet .info_complet_icon {
    display: block;
    float: left;
    width: 32px;
    height: 32px;
    background-image: url(../img/lay_index.png);
    background-repeat: no-repeat;
    overflow: hidden;
    position: relative;
    top: 6px;
    margin-right: 15px;
}

.info_complet .info_complet_icon.icon_1 {
    background-position: 0 -90px;
}

.info_complet .info_complet_icon.icon_2 {
    background-position: -497px -91px;
}

.info_complet .info_complet_icon.icon_3 {
    background-position: -164px -90px;
}

.info_complet .info_title {
    color: #666666;
    font-size: 18px;
    overflow: hidden;
    display: inline-block;
}

.info_complet .info {
    color: #333333;
    font-size: 18px;
    overflow: hidden;
    display: inline-block;
}

.info_nav .qd_info{
	float: right;
	margin-right: 80px;
}

.info_nav .qd_info img{
	width: 150px;
	height: 150px;
}

.info_nav .qd_info .qd_intro{
	width: 160px;
	text-align: center;
	font-size: 18px;
	color: #333;
	margin-top: 15px;
	font-weight: 600;
}

/*详细信息*/
.synopsis_complet{
	width: 90%;
	margin-bottom: 25px;
	width: 1100px;
}

.synopsis_complet:after{
	content: '';
    clear: both;
    display: table;
}

.synopsis_complet .synopsis_title{
	height: 35px;
	line-height: 35px;
	font-size: 18px;
	color: #333;
	margin-bottom: 15px;
	font-weight: 600;
}

.synopsis_complet .synopsis_text{
	width: 100%;
	position: relative;
}

.synopsis_text .editor{
	border: 1px solid #dddddd;
	padding: 10px;
	line-height: 1.5;
	font-size: 14px;
	color: #333;
	resize: none;
	margin-bottom: 0;
}

.synopsis_text .editor:focus{
	border-color: rgba(212,172,87,0.8);
    outline: 0;
    outline: thin dotted \9;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075), 0 0 8px rgba(212,172,87,0.8);;
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(212,172,87,0.8);;
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.075), 0 0 8px rgba(212,172,87,0.8);;
}


.synopsis_text .synopsis_textarea{
	width: 100%;
	height: 150px;
}

.synopsis_text .editor_icon{
	display: block;
	position: absolute;
	bottom: 10px;
	right: 10px;
	width: 25px;
	height: 25px;
	background: url(../img/lay_index.png) no-repeat -135px 0;
	cursor: pointer;
}

.synopsis_text .editor_icon:hover{
    background: url(../img/lay_index.png) no-repeat -176px 0;	
}

/*时间间隔*/
.synopsis_text .time_area{
	width: 50%;
	float: left;
	height: 60px;
	line-height: 60px;
}

.synopsis_text .time_area:last-child{
	padding-left: 50px;
}

.synopsis_text .time_area .title{
	font-size: 16px;
	color: #666;
	
}

.synopsis_text .time_area .time_input{
	height: 40px;
	margin-left: 10px;
	display: inline-block;
	width: 40%;
	position: relative;
	vertical-align: middle;
}

.synopsis_text .time_area .time_input .editor_icon{
	bottom: 6px;
	right: 10px;
}

.synopsis_text .time_area .time-control{
	padding: 5px 10px;
	width: 100%;
	height: 100%;
	vertical-align: top;
}

.synopsis_text .time_area .time_input.start{
	margin-right: 10px;
}

/*联系电话*/
.synopsis_text .phone_area{
	width: 48%;
	float: left;
	height: 60px;
	line-height: 60px;
	position: relative;
}

.synopsis_text .phone_area .editor_icon{
	bottom: 16px;
	right: 10px;
}

.synopsis_text .phone_area:first-child{
	margin-right: 4%;
}

.synopsis_text .phone_area .phone{
	height: 40px;
	padding: 5px 10px;
	width: 100%;
}

/*地区*/
input[type="text"].area_control{
	height: 40px;
	padding: 5px 10px;
	width: 15%;
	margin-right: 2%;
	border: 1px solid #dddddd;
    line-height: 1.5;
    font-size: 14px;
    color: #333;
    resize: none;
    background:#fff;
}

.details_nav{
	width: 48%;
	float: right;
	vertical-align: middle;
}

.details_nav > span{
	font-size: 18px;
	color: #666;
	float: left;
	width: 100px;
	height: 40px;
	line-height: 40px;
}

.details_nav .details_area{
	overflow: hidden;
	position: relative;	
}

.details_nav .details_area .details_control{
	height: 40px;
	padding: 5px 10px;
	width: 100%;
}

.details_area .editor_icon{
	bottom: 6px;
	right: 10px;
}
.synopsis_text input[type="text"]{
	margin-bottom:0px;
}

/*服务设置*/
.service_area{
	float: left;
	width: 60px;
	margin-top: 10px;
	margin-right: 60px;
	cursor: pointer;
}

.service_area > .service_icon{
	display: block;
	width: 60px;
	height: 60px;
	background-image: url(../img/lay_index.png);
	background-repeat: no-repeat;
}

.service_area:hover .service_icon{
	animation: shake 0.4s cubic-bezier(.36, .07, .19, .97) both;
    transform: translate3d(0, 0, 0);
}

@-webkit-keyframes shake {
	25%,
	75% {
		transform: translate3d(-2px, 0, 0);
	}
	50%,
	90% {
		transform: translate3d(2px, 0, 0);
	}
}

@keyframes shake {
	25%,
	75% {
		transform: translate3d(-2px, 0, 0);
	}
	50%,
	90% {
		transform: translate3d(2px, 0, 0);
	}
}

.service_area > .service_icon.icon_1{
	background-position: 3px -233px;
}

.service_area > .service_icon.icon_2{
	background-position: -77px -233px;
}

.service_area > .service_icon.icon_3{
	background-position: -152px -233px;
}

.service_area > .service_icon.icon_4{
	background-position: -224px -233px;
}

.service_area > .service_icon.icon_5{
	background-position: -299px -233px;
}

.service_area > .service_icon.icon_6{
	background-position: -369px -233px;
}




