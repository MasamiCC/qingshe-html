/*商家logo*/
.choose_logo_nav{
	float: left;
	width: 150px;
	cursor: pointer;
	margin-right: 70px;
}

.choose_logo_area{
	width: 150px;
	height: 150px;
	position: relative;	
}

.choose_logo_nav img{
	width: 150px;
	height: 150px;
}

.choose_logo_nav .file_input{
	position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 150px;
    filter: alpha(opacity=0);
    -moz-opacity: 0;
    -khtml-opacity: 0;
    opacity: 0;	
}

.logo_prompt > p{
	text-align: center;
	font-size: 16px;
	color: #333333;
	margin: 0;
	margin-top: 10px;
}

.logo_prompt > p:last-child{
	color: #999999;
	margin-top: 5px;
}

.choose_logo_area.has_img .operate_img{
	display: block;
}

.operate_img{
	display: none;
}

.operate_img .edit_img{
	position: absolute;
	width: 100%;
	height: 40px;
	line-height: 40px;
	bottom: 0;
	left: 0;
	background-color: rgba(0,0,0,0.8);
	color: #d4ac57;
	font-size: 14px;
	text-align: center;
}

.operate_img .remove_img{
	position: absolute;
	top: -10px;
	right: -10px;
	width: 25px;
	height: 25px;
	line-height: 25px;
	text-align: center;
	border-radius: 100%;
	background-color: #d4ac57;
	font-size: 20px;
	color: #fff;
}

/*全景图片*/
.panorama_area{
	width: 150px;
	height: 150px;
	overflow: hidden;
	position: relative;
	cursor: pointer;
}

.panorama_area img{
	width: 100%;
	height: 100%;
}

.panorama_area .enter_mange{
	position: absolute;
    width: 100%;
    height: 40px;
    line-height: 40px;
    bottom: 0px;
    left: 0;
    background-color: rgba(0,0,0,0.8);
    color: #d4ac57;
    font-size: 14px;
    text-align: center;
    -webkit-transition: .5s 0s all;
	-o-transition: .5s 0s all;
	-moz-transition: .5s 0s all;
	-ms-transition: .5s 0s all;
	transition: .5s 0s all;
}


.panorama_area .enter_mange:hover{
	text-decoration: none;
	background-color: rgba(0,0,0,1);
}



/*按钮区域*/
.btn_area{
	margin-top: 150px;
	text-align: center;
	margin-bottom: 150px;
}

.btn_area .submit_btn{
	border: none;
	display: inline-block;
	height: 65px;
	line-height: 65px;
	background-color: #221910;
