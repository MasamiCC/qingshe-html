<script type="text/javascript">
	$(function(){
		$('.collect_a').mouseover(function(){
			$('.collect_a').html('<i class="fa fa-star-o text-y"></i>取消').addClass('text-y')
		}).mouseleave(function(){
			$('.collect_a').html('<i class="fa fa-star-o text-y"></i>已收藏').removeClass('text-y')
		})
		$('.collect').click(function(){
			var hasC=$(this).hasClass('collect_a');
			if(hasC){
				alert('取消收藏')
			}else{
				alert('添加 收藏')
			}
		})
		$(".tousu").click(function(){
			alert('投诉')
		})
	})
	
</script>
