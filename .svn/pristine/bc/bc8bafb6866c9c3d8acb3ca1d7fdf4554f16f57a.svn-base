<script type="text/javascript">
	$(function(){
		$(window).scroll(function(){
			var top=$(window).scrollTop();
			var height=$(window).height();
			
			if ($(document).height()-top==height) {
				loadHistory()
			}
		})
	})
	var pageCount=1;
	var pageEnd=false;
	
	function loadHistory(){
		
		if (!pageEnd) {
			if (pageCount>=2) {
				pageEnd=true;
				mui.toast('没有更多数据了')
			}else{
				for( var i =0;i<10;i++){
					var html='<li class="mui-table-view-cell mui-media" data-id="13">'
				        +'<a href="javascript:;">'
				        +    '<img class="mui-media-object" src="http://www.592vip.com:90/stvideo/12/6e164690-67ff-4145-9c4a-81d829b4f16d.jpg">'
				        +    '<div class="mui-media-body history_list_name title">'
				        +        	'<div class="history_title">'
				        +       	 	'轻奢点评《奢探》直播之披萨达人'
				        +       	'</div>'
					    +        '<div class="grey">'
						+			'<span id="">05月05日</span>'
						+			'<span id=""><img class="watch" src="../image/watch.png"></span>'
						+			'<span>1150</span>'
						+		'</div>'
				        +    '</div>'
				        +'</a>'
				    	+'</li>'
					$("#history_list").append(html);
				}
				pageCount++;
			}
			
		}else{
			mui.toast('没有更多数据了')
		}
	}
	
</script>