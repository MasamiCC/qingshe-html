	right: -0.2rem;
	top: -1.4rem;
}

.btn_area .btn:first-child {
	margin-bottom: 1rem;
}


/*弹出框*/

.prompt_modle {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, .6);
	z-index: -1;
	padding: 0 1.5rem;
	box-sizing: border-box;
	display: none;
	z-index: 9;
}

.prompt_bg {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
}


/*收益提示*/

.evalute_prompt {
	padding: 1rem;
	background-color: #FFFFFF;
	border-radius: 1rem;
	position: relative;
	width: 100%;
	box-sizing: border-box;
}


/*关闭按钮*/

.close-box {
	position: absolute;
	right: 1rem;
	top: 1rem;
	width: 30px;
	height: 30px;
	z-index: 99999;
}

a.close {
	display: inline-block;
	width: 30px;
	height: 2px;
	background: #e0e0e0;
	line-height: 0;
	font-size: 0;
	vertical-align: middle;
	-webkit-transform: rotate(45deg);
	position: absolute;
	right: 0;
	top: 15px;
}

a.close:after {
	content: '/';
	display: block;
	width: 30px;
	height: 2px;
	background: #e0e0e0;
	-webkit-transform: rotate(-90deg);
}


/*内容*/

.prompt_content {
	color: #333333;
	text-align: center;
}

.prompt_title {
	font-size: 22px;
	margin-top: 1.5rem;
}

.prompt_text {
	font-size: 1rem;
	margin-top: 1rem;
	line-height: 2;
}

.prompt_text .big {
	font-size: 1.2rem;
}

.prompt_content .btn_area {
	padding: 0;
}


/*
 * 底部弹出框
 */
.cus_popover {
    position: fixed;
    top: 100%;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    text-align: center;
    z-index: 999;
    border-radius: 7px;
    background-color: #f7f7f7;
    padding: 1.5rem;
    box-sizing: border-box;
    padding-bottom: 2rem; 
}

.cus_popover *{
	box-sizing: border-box;
}

.cus-popover-cont {
	display: block;
	padding-bottom: 30px;
	overflow-y: auto;
	height: 100%;
}

.cus-popover-cont>div {
	padding: 0.3rem 0;
}

.cus-popover-cont .cus-name{
	font-size: 18px;
	color: #000000;
	font-weight: bold;
}

.cus-table table {
	margin: 0 auto;
}

.cus-table table {
	border-collapse: separate;
	border-spacing: 5px;
	margin: 0 auto;
}

.cus-table th,
.cus-table td {
	background-color: #eeeeee;
	font-size: 0.8rem;
	vertical-align: middle;
	padding: 0.5rem 0;
	color: #333;
	font-weight: normal;
}

.cus-img > img{
	width: 80px;
	height: auto;
}

.cus-popover-cont .cus_msg{
	text-align: left;
	font-size: 14px;
}

/*遮罩层*/
.popover_bg{
	position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    overflow: hidden;
    background: rgba(0,0,0,0.5);
    display: none;
}

.cus_popover .btn_area{
	margin-top: 1rem;
}


