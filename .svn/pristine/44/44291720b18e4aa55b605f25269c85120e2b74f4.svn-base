*{
	padding: 0;
	margin: 0;
}
/*head*/
.qingshehao_all,body{
	background: #fff;
}
.qingshehao_all1{
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin: 0 15px;
	font-size: 0.75rem;
	color: #fff;
}
.qingshehao_head{
	height: 275px;
	position: relative;
	background: url(../image/qingshehaobg.png) no-repeat;
	background-size: 100% 100%;
}
.qingshehao_all1_item{
	display: flex;
	line-height: 2.5;
	flex-direction: column;
}
.qingshehao_all1_itemitem{
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
/*bar*/
.etr_item_banner_middle{
	position: relative;
	width: 95%;
	margin: 10px auto;
	text-align:center;
	height: 10px;
}
.bar_left{
	position: absolute;
	width: 0;
	height: 8px;
	background: #12c91f;
	border-radius: 5px;
	left: 0;
	transition: width 2s;
	-moz-transition: width 2s;	/* Firefox 4 */
	-webkit-transition: width 2s;	/* Safari å’Œ Chrome */
	-o-transition: width 2s;
	z-index: 2;
}
.bar_right{
	position: absolute;
	width: 100%;
	height: 8px;
	background: #ddd;
	border-radius: 5px;
}
/*bar*/
.point_icon img{
	width: 15px;
	height: 16px;
}
.heart_icon img{
	width: 14px;
	height: 15px;
}
.point_icon{
	margin-right: 10px;
}
.point_icon{
	display: flex;
	justify-content: center;
	align-items: center;
}
.qingshehao_all2{
	line-height: 0;
    position: absolute;
    z-index: 2;
    bottom: 0;
    left: 0;
}
.qingshehao_all2_item2{
	line-height: 0;
	position: absolute;
	z-index: 3;
}
.qingshehao_all2 img{
	width: 230px;
    height: 168px;
}
.qingshehao_all3 img{
	height: 168px;
}
.qingshehao_all2_item1{
	margin-top: 17%;	
}
.qingshehao_all3{
	display: flex;
	padding: 0 15px;
	color: #ffe8a3;
	font-size: 0.8125rem;
	background: url(../image/huochedao.png) no-repeat;
	background-size: 100% 100%;
	line-height: 3;
	padding-bottom: 10px;
}
.qingshehao_all3_item1{
	flex: 1;
}
.qingshehao_all3_item2{
	flex: 2;
}
.qingshehao_all3>div{
	display: flex;
	justify-content: center;
	align-items: center;
}
.qingshehao_all4 img{
    width: 217px;
    height: 70px;
}
.qingshehao_all5 img{
    width: 125px;
    height: 80px;
}
.qingshehao_all4,.qingshehao_all5{
	position: absolute;
    bottom: 0;
    line-height: 0;
    z-index: 5;
}
.qingshehao_all5{
	bottom: 59px;
    left: 104px;
}
/*head*/
/*gonglue*/
.gonglue_all_line{
	display: flex;
    margin: 15px;
    padding: 5px 15px;
    border: 3px solid #c5eaff;
    border-radius: 7px;
    box-shadow: 2px 2px 1px #eee;
}
.gonglue_line_left{
	flex: 3;
}
.gonglue_line_right{
	flex: 1;
	justify-content: center;
	align-items: center;
	display: flex;
}
.gonglue_line{
	position: relative;
    line-height: 2.5;
    padding-left: 5px;
    font-size: 1.125rem;
    display: flex;
    align-items: center;
}
.blue_icon{
	display: inline-block;
    width: 5px;
    height: 5px;
    border-radius: 50%;
    background: #72b1ff;
    margin-right: 10px;
}
.icon1:after{
    content: "";
    height: 47%;
    /* line-height: 1; */
    border: 0.3px dashed #72b1ff;
    position: absolute;
    top: 74%;
    left: 7px;
}
.gonglue_icon{
	padding: 7px 20px;
    color: #fff;
    background: -moz-linear-gradient(top, #47d0ff, #2298f9);
    background: -o-linear-gradient(top,#47d0ff, #2298f9);
    background: -webkit-gradient(linear, 0% 100%, 100% 100%, from(#47d0ff), to(#2298f9));
    border-radius: 18px;
    font-size: 1rem;
}
.grey_font{
	color: #999;
    margin-left: 10px;
    font-size: 0.8125rem;
}
.displaylb{
	display: inline-block;
}
/*gonglue*/
/*point*/
.point_title_icon img{
	width: 20px;
}
.qingshehao_all_point{
	margin: 0 15px;
}
.point_title_icon{
	margin-right: 5px;
}
.point_title{
	font-size: 1.125rem;
	font-weight: bold;
	margin-bottom: 10px;
}
.point_small_table_head{
	display: flex;
    justify-content: space-between;
    background: #f4f4f4;
    padding: 0 20px;
    line-height: 2;
    font-size: 0.875rem;
    color: #999;
    margin-bottom: 10px;
}
.point_small_table_line2img img{
	width: 26px;
	height: 26px;
	border-radius: 50%;
}
.point_small_table_line2img{
	margin-right: 10px;
}
.point_small_table table{
	width: 100%;
	text-align: center;
}
.tdlist1{
	width: 70px;
}
.tdlist2{
	width: 64%;
}
.table_head,.table_name{
	vertical-align: middle;
	display: inline-block;
}
.point_small_table table tr{
	line-height: 2.5;
}
