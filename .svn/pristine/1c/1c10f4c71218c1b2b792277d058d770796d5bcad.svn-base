			}
			.empty{
				clear: both;
			}
			.trajectory_detail img{
				max-width: 100%;
			}
			.trajectory_item hr{
				width: 25px;vertical-align: middle;margin-top:10px;margin-right: 5px;border: none;height: 1px;color: #eee;background: #ccc;
			}
			.trajectory_btn{
				display: flex;align-items: center;
