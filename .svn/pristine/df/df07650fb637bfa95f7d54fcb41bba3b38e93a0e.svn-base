*{padding: 0;margin: 0;list-style: none;}
			body,.mui-content{
				background: #f4f4f4;
			}
			.mui-content{
				margin-bottom: 55px;
			}
			.uc_header{
				font-size: 0;line-height: 0;background: url(../image/uc_bg@2x.png);
				position: relative;
				padding-top: 2rem;
				padding-bottom: 2rem;
				background-size: cover;
				background-repeat: no-repeat;
				background-position: bottom right; 
			}
			
			.uc_header_user ul li img{
				width: 55%;
			
			}
			.uc_name{line-height: 1.8;font-size: 1.15rem;}
			.uc_header_check{
				position: absolute;right: 0;bottom: 0;width: 25%;
				text-align: right;
			}
			.uc_header_check img{
				width: 100%;max-width: 155px;
			}
			.uc_header_user{display: flex;align-items: center;font-size: 1rem;color: #fff;margin: 0 1rem;}
			.uc_header_user >div:nth-child(1){
				width: 5rem;text-align: center;
			}
			.uc_header_user >div:nth-child(1) img{
				width: 4rem;height: 4rem;border-radius: 100%;margin: 0 auto;
			}
			.uc_menu{line-height: 0;}
			.uc_menu .mui-grid-view.mui-grid-9{
				background-color: #fff;
			}
			.uc_menu .mui-grid-view.mui-grid-9 .mui-table-view-cell{
				padding: 0;
			}
			.uc_menu img{
				width:30%;
			}
			.uc_menu .mui-grid-view.mui-grid-9:after, .mui-grid-view.mui-grid-9:before{
				height: 0;
			}
			.mui-badge-danger, .mui-badge-red{
				padding: 4px;position: absolute;
			}
			.uc_banner{font-size: 0;line-height: 0;text-align: center;}
			.uc_banner img{
				max-width: 100%;margin: 0 auto;
			}