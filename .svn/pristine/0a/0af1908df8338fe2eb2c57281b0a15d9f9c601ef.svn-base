/*rotate*/
@-webkit-keyframes rotater{
	0%{transform:rotate(0);}
	100%{transform:rotate(360deg);}
}
@-moz-keyframes rotater{
	0%{transform:rotate(0);}
	100%{transform:rotate(360deg);}
}
@-ms-keyframes rotater{
	0%{transform:rotate(0);}
	100%{transform:rotate(360deg);}
}
@keyframes rotater{
	0%{transform:rotate(0);}
	100%{transform:rotate(360deg);}
}
@-webkit-keyframes rotater2{
	0%{transform:rotate(0);}
	100%{transform:rotate(-360deg);}
}
@-moz-keyframes rotater2{
	0%{transform:rotate(0);}
	100%{transform:rotate(-360deg);}
}
@-ms-keyframes rotater2{
	0%{transform:rotate(0);}
	100%{transform:rotate(-360deg);}
}
@keyframes rotater2{
	0%{transform:rotate(0);}
	100%{transform:rotate(-360deg);}
}
