	height: 40px;line-height: 40px;
	padding: 0 10px;
	/*background: #DDDDDD;*/
	/*color: #666;*/
}
.pingjia_title span{
	float: right;
}
.pingjia_person img{
	width: 60px;height: 60px;vertical-align: middle;display: block;
}
.pingjia_cont{
	line-height: 25px;font-size: 14px;color: #666;padding: 10px ;letter-spacing: 1px;
	overflow: hidden;
}
.pingjia_con {
	/*max-width: 795px;*/
	width: 94%;
}
.pingjia_cont textarea{
	margin: 10px auto;resize: none;padding: 10px 10px;border-radius: 5px;
	width: 99%;
}
.pingjia_re_btn,.pingjia_re_sub{
	border:none;
	padding: 3px 10px ;
	background: #D4AC57;
	color: #fff;border-radius: 3px;text-decoration: none;
}
.pingjia_re_sub{
	float: right;
}
.pingjia_re_btn:hover{
	color: #fff;text-decoration: none;
}
.pingjia_form{
	display: none;
}
