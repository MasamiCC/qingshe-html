.title {
	flex: 0;
}

.title_content {
	display: flex;
	justify-content: center;
	align-items: center;
}

.title_txt {
	font-size: 1.4rem;
	font-weight: 500;
	letter-spacing: 1px;
	color: #333333;
	font-weight: bold;
}

.title_content>img {
	height: 5rem;
	width: auto;
	padding: 0 20px;
}


/*收益对比*/

.compare_area {
	padding: 1rem;
	background-color: #feeedf;
}

.compare_title {
	letter-spacing: 1px;
	color: gray;
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: .9rem;
	height: 1.5rem;
	line-height: 1.5rem;
}

.compare_result {
	color: #333333;
	font-weight: bold;
}

.compare_result .win {
	font-size: .9rem;
	color: #FFFFFF;
	display: inline-block;
	width: 25px;
	height: 25px;
	text-align: center;
	align-items: center;
	border-radius: 99px;
	margin-right: 5px;
	font-weight: normal;
}

.compare_result.b_win .win {
	background: #f8a120;
}

.compare_result.a_win .win {
	background: #d0d0d0;
}

/*比较*/
.compare_nav{
	overflow: hidden;
	margin: .4rem 0;
}

.compare_nav *{
	box-sizing: border-box;
}

.compare_nav .compare {
	height: 2rem;
}

.compare_coord{
	float: left;
	font-size: 14px;
	color: #333333;
	border-right: 1px solid #936e5c;
	padding-right: .3rem;
	height: 2rem;
	line-height: 2rem;
}

.compare_coord > span{
	display: inline-block;
	width: 10px;
}

.compare_bar{
	overflow: hidden;
	height: 2rem;
	padding: .5rem 0;
	display: flex;
	display: -webkit-flex;
	
}
.compare_bar .text{
	font-size: 14px;
	color: #323232;
	margin-left: .5rem;
}

.compare_bar > div{
	display: inline-block;
}

/*进度条A*/
.compare_a .compare_bar .progress{
	height: 100%;
	background-color: #d0d0d0;
}

.compare_b .compare_bar .progress{
	height: 100%;
	background-color: #f8a120;
}

/*路径3*/
.compare_3{
	background-color: #FFFFFF;
	border-left: 1px solid #936e5c;
	border-bottom: 1px solid #936e5c;
	height: 10rem;
	padding-top: .5rem;
	position: relative;
}

.compare_nav.compare_3 .compare{
	height: 0;
	width: 50%;
	text-align: center;
	float: left;
	position: relative;
}

.compare_nav.compare_3 .compare_bar{
	padding: 0;
	height: 100%;
}

.compare_nav.compare_3  .pillar{
	width: 60%;
	margin: 0 auto;
	position: relative;
}

.compare_nav.compare_3  .pillar .text{
	height: 20px;
	line-height: 20px;
}

.compare_nav.compare_3  .pillar .progress{
	width: 100%;
	height: 100%;
}

/*虚线*/
.compare_3 .dashed{
	height: 1px;
	width: 100%;
	position: absolute;
	bottom: 0px;
	border-top: 1px dashed #000000;
}


.compare_prompt {
	font-size: .9rem;
	color: #000000;
	line-height: 1.5rem;
	height: 1.5rem;
	font-weight: bold;
}

.compare_prompt>span {
	font-size: 1.1rem;
}


/*红利黑钻对比*/

.compare_area.black_compare {
