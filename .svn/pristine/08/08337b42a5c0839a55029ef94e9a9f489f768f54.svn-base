			/*bar*/
			.etr_item_banner_middle{
				position: relative;
				width: 54%;
				margin: 10px auto 25px auto;
				text-align:center;
				height: 8px;
			}
			.bar_left{
				position: absolute;
				width: 0;
				height: 8px;
				background: #f8a120;
				border-radius: 5px;
				/*border-bottom-right-radius: 0;
				border-top-right-radius: 0;*/
				left: 0;
				transition: width 2s;
				-moz-transition: width 2s;	/* Firefox 4 */
				-webkit-transition: width 2s;	/* Safari 和 Chrome */
				-o-transition: width 2s;
				z-index: 2;
			}
			.bar_right{
				position: absolute;
				width: 100%;
				height: 8px;
				background: #eee;
				border-radius: 5px;
			}
			/*bar*/
			@media only screen and (min-width: 376px) and (max-width: 414px) {
