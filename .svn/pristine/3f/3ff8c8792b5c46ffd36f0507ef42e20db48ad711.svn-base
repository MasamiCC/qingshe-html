#t_popover .mui-icon{position: absolute;z-index: 999;left: 0;right: 0;margin: 0 auto;text-align: center;bottom: -100px;color: #fff;font-size: 1.5rem;}



/*2017.12.12*/
.newYear{background: url(../image/newyear_bg.png);background-repeat: no-repeat;background-size: contain;background-color: #fff;}
.newYear_title{position: absolute;left: 0;right: 0;text-align: center;top: 1.8rem;color: #fff;font-size:0.875rem;}
@media only screen  and (max-width: 320px) {
	.newYear_title{top: 1.5rem;}
}
@media only screen  and (min-width: 414px) {
	.newYear_title{top: 2rem;}
}
.newYear_shake {
	position: absolute;right: 1.5rem;top: 1rem;
	animation: myfirst 2s infinite alternate;
	-moz-animation: myfirst 2s infinite alternate;	/* Firefox */
	-webkit-animation: myfirst 2s infinite alternate;	/* Safari 和 Chrome */
	-o-animation: myfirst 2s infinite alternate;
}
.newYear_shake img{width: 90%;}
@keyframes myfirst
{
	from {top: 0.5rem;}
	to {top: 2rem;}
}

@-moz-keyframes myfirst /* Firefox */
{
from {top: 0.5rem;}
	to {top: 2rem;}
}

@-webkit-keyframes myfirst /* Safari 和 Chrome */
{
from {top: 0.5rem;}
	to {top: 2rem;}
}

@-o-keyframes myfirst /* Opera */
{
from {top: 0.5rem;}
	to {top: 2rem;}
}