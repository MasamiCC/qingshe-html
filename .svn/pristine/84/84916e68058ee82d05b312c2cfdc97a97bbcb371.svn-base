<script src="../js/mui.min.js"></script>
<script src="../js/jquery-1.10.1.min.js" type="text/javascript" charset="utf-8"></script>
<script src="../js/swiper.min.js" type="text/javascript" charset="utf-8"></script>
<script src="../js/mui.picker.js" type="text/javascript" charset="utf-8"></script>
<script src="../js/mui.poppicker.js" type="text/javascript" charset="utf-8"></script>
<script src="../js/city.data-3.js" type="text/javascript" charset="utf-8"></script>
<script src="../js/common.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
	(function($, doc) {
		$.init();
		$.ready(function() {
			//普通示例
			
			
			//-----------------------------------------
			//					//级联示例
			var cityPicker3 = new $.PopPicker({
				layer: 3
			});
			cityPicker3.setData(createData(cityData));
			var showCityPickerButton = doc.getElementById('changeCity');
			var cityResult3 = doc.getElementById('changeCity');
			showCityPickerButton.addEventListener('tap', function(event) {
				cityPicker3.show(function(items) {
					cityResult3.value = (items[0] || {}).text + " " + (items[1] || {}).text + " " + (items[2] || {}).text;
					//返回 false 可以阻止选择框的关闭
					//return false;
				});
			}, false);
		});
	})(mui, document);
	var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: true,
        direction: 'vertical',
        noSwiping : true,
    });
    var mytimer='';
	$(function(){
		var step=sessionStorage.getItem("read");
		console.log(step)
		if(step==null){
			mui('#popover').popover('show')
		}
	    $('.slide_next').click(function(){
	    	var index=$(this).parents('.swiper-slide').index();
	    	if (!$(this).hasClass('slide_end')) {
	    		if(index==1){
		    		if(formCheck()){
		    			mySlideNext();
		    		}else{
		    			return false;
		    		}
		    	}else{
		    		mySlideNext();
		    	}
	    		
	    	}
	    	
	    	
	    })
	    $('.slide_prev').click(function(){
	    	mySlidePrev();
	    })
	    $("#closePop").on('tap',function(){
	    	mui('#popover').popover('hide');
	    	sessionStorage.setItem("read", "1");
	    })
	    $("#demopopover").on('tap','span',function(){
	    	mui('#demopopover').popover('hide')
	    })
	    $("#showDemo").on('tap',function(){
	    	mui('#demopopover').popover('show')
	    })
	    $(".add_person").on('tap',function(){
	    	
	    	var pSize=$('.person').size();
	    	if(pSize<6){
	    		var html='<div class="person">'
        				+	'<div class="step_title_s"><i class="fa fa-users"></i><span>家属</span><i class="mui-pull-right delPerson fa fa-trash" onclick="delPerson(this)"></i></div>'
        				+	'<div class="mui-input-row">'
        				+		'<label>姓名</label>'
            			+        '<div class="form_item"><input type="text" class="" placeholder="请输入家属姓名" name="name" data-title="姓名"></div>'
            			+    '</div>'
            			+    '<div class="mui-input-row">'
            			+        '<label>关系</label>'
            			+        '<div class="form_item"><input type="text" class="" placeholder="与申请人关系" name="relationship" data-title="与申请人关系"></div>'
            			+    '</div>'
            			+    '<div class="mui-input-row">'
            			+        '<label>工作单位</label>'
            			+        '<div class="form_item"><input type="text" class="" placeholder="请输入工作单位或无业、务农" name="workplace" data-title="工作单位"></div>'
            			+    '</div>'
            			+    '<div class="mui-input-row">'
            			+        '<label>联系方式</label>'
            			+        '<div class="form_item"><input type="text" class="" placeholder="请输入联系电话" name="phone" maxlength="11" data-title="联系方式"></div>'
            			+    '</div>'
            			+    '<div class="mui-input-row">'
            			+        '<label>月收入</label>'
            			+        '<div class="form_item"><input type="text" class="" placeholder="请输入月收入" name="income" data-title="月收入"><span>元</span></div>'
            			+    '</div>'
        				+'</div>';
        		$(this).before(html);
        		loadPersonTitle();
        		if(pSize==5){$(".add_person").hide()}
	    	}

	    })
	    $("#code").on('tap',function(){
	    	var clicked=$(this).hasClass('clicked');
	    	if(!clicked){
	    		$(this).addClass('clicked');
	    		var mobile=$("#myMobile").val();
	    		if(!(/^1[3|4|5|7|8][0-9]\d{8}$/.test(mobile))){
	    			mui.alert('手机号码格式不正确');
	    			$("#myMobile").focus();
	    			$(this).removeClass('clicked')
	    		}else{
	    			countTime("#code",10)
	    		}
	    	}
	    })
	    stepThirdClick()
	    mySlideTo(2)
	})
	
    function mySlideTo(index){
    	swiper.slideTo(index, 1000, false)
    }
    function mySlideNext(){
    	swiper.slideNext(1000,false);
    }
    function mySlidePrev(){
    	swiper.slidePrev(1000,false);
    }
    function stepThirdClick(){
    	$('.mui-row input[type=radio]').each(function(){
    		$(this).on('click',function(){
    			var siblings=$(this).parents('.dc_a_list').siblings().size();
    			var index=$(this).parent().index();
    			if(siblings==2){
    				if(index==0){
    					$(this).parents('.dc_a_list').next().slideDown()
    				}else{
    					$(this).parents('.dc_a_list').next().slideUp()
    				}
    			}
    			console.log($(this).parents('.dc_a_list').siblings().size())
    		})
    	})
    }
    function countTime(ele,count){
    	clearInterval(mytimer);
    	mytimer=setInterval(function(){
    		if (count>0) {
    			$(ele).html(count+"s后重试");
    			count--;
    		}else{
    			clearInterval(mytimer);
    			$(ele).removeClass('clicked');
    			$(ele).html("获取验证码");
    		}
    	},1000)
    }
    function changImg(me){
    	console.log(me)
    	$(me).parents('li').find('input[type=file]').click()
    }
    function change (obj) {
	    if (!obj.files.length) return;
	    if (obj.files.length > 1) {
	        mui.alert("只允许上传一张图片!");
	        return false;
	    }
	
	    var file = obj.files[0];
	
	    var reader = new FileReader();
	    reader.onload = function () {
	        var result = this.result;    //data:base64     
	        $(obj).parent().find('img').attr({
	        	src:result
	        })
	        $(obj).parent().find('div').append('<i class="fa fa-times-circle fa-2x" onclick="delImg(this)"></i>')
	        $(obj).parent().find('img').css({
	        	height:($(obj).parent().width()-10)*112/178,
	        	width:$(obj).parent().width()-10
	        })
	    };
	    reader.readAsDataURL(file);
	}
    function delImg(me){
    	$(me).parent().html('<img src="../image/social_add.png" onclick="changImg(this)">')
    }
    function delPerson(me){
    	$(me).parents('.person').remove();
    	loadPersonTitle();
    	if($('.person').size()<6){
    		$(".add_person").show()
    	}
    }
    function loadPersonTitle(){
    	var arr=['一','二','三','四','五'];
    	$('.person').each(function(){
    		var index=$(this).index();
    		if(index>1){
    			$(this).find('.step_title_s span').html('家属'+arr[index-1])
    		}
    	})
    }
    
    var applyFamily={};
    function formCheck(){
    	var err={};
    	$(".person").each(function(){
    		var _this=$(this);
    		var index=_this.index();
    		var keys=_this.find('.step_title_s span').text();
    		var flist=_this.find('.mui-input-row input');
    		var temp={};
    		var errTemp={};
    		flist.each(function(){
    			temp[$(this).attr('name')]=$(this).val();
    			console.log($(this).attr('name')+":"+$(this).val())
    			if($(this).val()==''){
    				errTemp[$(this).parents('.mui-input-row').index()]=$(this).attr('data-title')+'为空';
    			}
    		})
    		applyFamily[index]=temp;
    		if(JSON.stringify(errTemp)!='{}'){
    			err[keys]=errTemp;
    		}
    		
    	})
    	console.log(err)
    	if(JSON.stringify(err)!='{}'){
    		var str='';
    		for (var i in err) {
    			str+="<b>"+i+"</b>\r\n";
    			var e=err[i];
    			for (var j in e) {
    				str+=e[j]+"\r\n";
    			}
    		}
    		mui.alert(str);
    		return false;
    	}else{
    		return applyFamily;
    	}
    	
    }
    
    
    function thirdForm(){
    	$('.mui-row').each(function(){
    		var index=$(this).index();
    		console.log(index+1)
    	})
    }
</script>