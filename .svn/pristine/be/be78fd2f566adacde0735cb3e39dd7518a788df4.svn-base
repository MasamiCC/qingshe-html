*{margin: 0;padding: 0;list-style: none;outline: none;}
body{
	background: #e84747;
}
.mui-content{background: none;max-width: 600px;margin: 0 auto;}
.mui-bar-nav{box-shadow: none;}
.newYear_logo{font-size: 0;line-height: 0;text-align: center;position: relative;}
.newYear_logo img{max-width: 100%;display: block;margin: 0 auto;}
.newYear_gl{position: absolute;top: 42%;right: 0.875rem;width: 50px;}
.newYear_tickets{margin: 0.75rem 0.5rem;}
.newYear_tickets ul{font-size: 0;line-height: 0;}
.newYear_tickets li{display: inline-block;width: 33.3%;vertical-align: top;}
.newYear_tickets li >div{margin: 0 2px;position: relative;overflow: hidden;}
.newYear_tickets li:first-child>div{margin-left: 0;margin-right: 4px;}
.newYear_tickets li:last-child >div{margin-right: 0;margin-left: 4px;}
.newYear_tickets li >div>img{max-width: 100%;}
.newYear_tickets li >div span{position: absolute;z-index: 99;display: block;top: 0;bottom:0;right: 0;width: 100%;text-align: right;background: rgba(0,0,0,0.2);height: 100%;font-size: 0;line-height: 0;}
.newYear_tickets li >div span img{max-height:85%;margin-top: 5%;margin-right: 0.5rem;}
.newYear_shops_title img{max-width: 100%;}
.newYear_shop_list{font-size: 0;margin: 0 0.5rem;}
.newYear_shop{display: inline-block;width: 25%;vertical-align: top;float: left;}
.newYear_shop:nth-child(1){width: 50%;}
.newYear_shop:nth-child(2),.newYear_shop:nth-child(3){width: 50%;}
.newYear_shop>div{margin: 0.5rem 2px;position: relative;}
.newYear_shop:nth-child(1)>div{margin: 0 3px 0 0;}
.newYear_shop:nth-child(2)>div,.newYear_shop:nth-child(3)>div{margin: 0 0 6px  3px;}
.newYear_shop:nth-child(4)>div{margin-left:0 ;margin-right: 4px;}
.newYear_shop:nth-child(7)>div{margin-left:4px ;margin-right: 0;}
.newYear_shop_img{line-height: 0;text-align: center;}
.newYear_shop_img img{max-width: 100%;margin: 0 auto;}
.newYear_shop_pro{background: #fd4684;padding-bottom: 0.3rem;}
.newYear_shop_pro div{
	color: #fff;font-size: 0.875rem;text-indent: 5px;line-height: 1.8;
	overflow:hidden;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-webkit-text-overflow:ellipsis;
	-moz-text-overflow:ellipsis;
	white-space:nowrap;
	}
.newYear_shop_pro p{color: #ffd451;font-size: 0.75rem;text-indent: 5px;margin-bottom: 0;line-height: 1.5;    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;}
.newYear_shop:nth-child(1) .newYear_shop_pro{position: absolute;width: 80%;left: 0;right: 0;margin: 0 auto;border-radius: 3rem;bottom:0.75rem;padding-left: 0.75rem;}
.newYear_shop:nth-child(1) .newYear_shop_pro:after{font-size: 32px;right: 0;color: #fff;}
.newYear_shop:nth-child(2) .newYear_shop_pro,.newYear_shop:nth-child(3) .newYear_shop_pro{
	position: absolute;bottom:3rem;width: 60%;right: 0;background: none;
}
.newYear_shop:nth-child(2) .newYear_shop_pro div,.newYear_shop:nth-child(3) .newYear_shop_pro div{
	font-size: 0.9rem;font-weight: 500;line-height: 1.5;
	
}
.newYear_shop:nth-child(2) .newYear_shop_pro p,.newYear_shop:nth-child(3) .newYear_shop_pro p{
	font-size: 0.8rem;color: #fff;line-height: 1;
}
.newYear_shop:nth-child(2) .newYear_shop_go,.newYear_shop:nth-child(3) .newYear_shop_go{
	bottom:0.875rem;position: absolute;width: 35%;right: 0.75rem;
	background: #fd4684;color: #fff;font-size: 1rem;
	border-radius: 2rem;
	padding:  0.3rem 0.5rem; 
}
.newYear_shop:nth-child(2) .newYear_shop_go:after,.newYear_shop:nth-child(3) .newYear_shop_go:after{
	font-size: 18px;right: 2px;
	color: #fff;

}
.newYear_date{
	margin:  0.5rem;
	filter:alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=red,endcolorstr=blue,gradientType=0);
    -ms-filter:alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=red,endcolorstr=blue,gradientType=0);/*IE8*/	
    background:#fff8b0; /* 一些不支持背景渐变的浏览器 */  
    background:-moz-linear-gradient(top, #fff8ae, #fffac4);  
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#fff8ae), to(#fffac4));  
    background:-o-linear-gradient(top, #fff8ae, #fffac4); 
    padding: 5px 0;
}
.newYear_date ul{font-size: 0;}
.newYear_date li{display: inline-block;color: #333;width: 12.5%;font-size: 0.875rem;vertical-align: middle;text-align: center;line-height: 1.5;font-weight: 500;}
.newYear_date li.active{
	color: #ff6600;
	
}
.newYear_gets{margin-bottom: 10px;}
.newYear_gets li{display: inline-block;background: rgba(0,0,0,0.5);padding: 0.2rem 0.75rem  0.2rem 0.3rem ;border-radius: 2rem;color: #fff;font-size: 0.875rem;margin: 0 0.2rem;}
.newYear_gets li>div{vertical-align: middle;}
.newYear_gets li img{vertical-align: middle;max-width: 2rem!important;border-radius: 3rem;width: 2rem;height: 2rem;}
.newYear_gets li span{color: #ff6600;}
.str_wrap{line-height: 2;background: none!important;}



.newYear_t{margin:0.75rem auto;position: relative;}
.newYear_t_bg img{max-width: 100%;}
#discount{position: absolute;top: 12%;left: 8%;}
@media only screen and  (max-width: 320px) {
	#discount{left: 8%;top: 15%;}
	.flipUnitContainer{
	    width: 45px;
		height: 60px;
	}
	.flipCard{
		width: 35px;
		height: 25px;
	}
	.upperCard span, .lowerCard span{font-size: 3rem;}
	.flipCard span{font-size: 3rem;}
	span.flipUnitContainer{width: 15px;height: 15px;}
}
@media only screen and  (min-width: 320px) {
	#discount{left: 7%;top: 15%;}
}

